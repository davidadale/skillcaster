<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p="urn:import:com.pietschy.gwt.pectin.client.components">
	<ui:style>
		.category {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<div>
			<g:Label addStyleNames="{style.category}" ui:field="category"></g:Label>
		</div>
		<div>
			<g:TextArea ui:field="qualifications"></g:TextArea>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 